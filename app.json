{
  "expo": {
    "name": "BaitMeOut",
    "slug": "BaitMeOut",
    "description": "Bait Me Out is simply a Truth Question Randomiser. Good truth questions are thought-provoking, and some can elicit just as much of an adrenaline rush as doing a dare. Struggling to come up with a question? just hit 'Bait Me Out' and let the app handle the rest!",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "BaitMeOut",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#2A1334"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shavgraphics.baitmeout",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2A1334"
      },
      "package": "com.shavgraphics.baitmeout",
      "versionCode": 2
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "notification": {
      "iosDisplayInForeground": true
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f49eb4e0-a1f7-448a-86d9-0fd18e3d374e"
      }
    }
  }
}
